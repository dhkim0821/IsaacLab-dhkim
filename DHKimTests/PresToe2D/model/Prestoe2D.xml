<mujoco model="Prestoe2D">
<visual>
  <global offwidth="1200" offheight="900" elevation="-120" azimuth="120"/>
</visual>
  <compiler angle="degree" inertiafromgeom="true"/>
  <default>
    <joint armature="0.01" damping=".1" limited="true"/>
    <geom conaffinity="0" condim="3" contype="1" density="1000" friction=".7 .1 .1" rgba="0.8 0.6 .4 1"/>
  </default>
  <option integrator="RK4" timestep="0.002"/>
  <worldbody>
    <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
    <geom conaffinity="1" condim="3" name="floor" pos="0 0 0" rgba="0.8 0.9 0.8 1" size="40 40 40" type="plane" material="MatPlane"/>
    <!-- <body name="torso" pos="0 0 1.25"> -->
    <body name="torso" pos="0 0 0">
      <camera name="track" mode="trackcom" pos="0 -3 -0.25" xyaxes="1 0 0 0 0 1"/>
      <joint armature="0" axis="1 0 0" damping="0" limited="false" name="rootx" pos="0 0 0" stiffness="0" type="slide"/>
      <joint armature="0" axis="0 0 1" damping="0" limited="false" name="rootz" pos="0 0 0" stiffness="0" type="slide"/>
      <joint armature="0" axis="0 1 0" damping="0" limited="false" name="rooty" pos="0 0 0" stiffness="0" type="hinge"/>
      <geom friction="0.9" name="torso_geom" pos="0 0 0.04" size="0.07 0.25" type="capsule"/>

      <body name="r_thigh" pos="0 0 -0.21">
        <joint axis="0 1 0" name="r_hip" pos="0 0 0" range="-150 150" type="hinge"/>
        <geom friction="0.9" pos="0 0 -0.174" name="thigh_geom" size="0.05 0.174" type="capsule"/>

        <body name="r_leg" pos="0 0 -0.348">
          <joint axis="0 1 0" name="r_knee" pos="0 0 0" range="0 150" type="hinge"/>
          <geom friction="0.9" name="leg_geom" pos="0 0 -0.1775" size="0.04 0.1775" type="capsule"/>

          <body name="r_foot" pos="0 0 -0.355">
            <joint axis="0 1 0" name="r_ankle" pos="0 0 0" range="-45 45" type="hinge"/>
            <geom friction="1.9" pos="0.05 0 -0.08" quat="0.70710678118654757 0 -0.70710678118654746 0" name="foot_geom" size="0.06 0.1" type="capsule"/>
          </body>
        </body>
      </body>
      <!-- copied and then replace thigh_left, leg_left, foot_left -->
      <body name="l_thigh" pos="0 0 -0.21">
        <joint axis="0 1 0" name="l_hip" pos="0 0 0" range="-150 150" type="hinge"/>
        <geom friction="0.9" name="thigh_left_geom" rgba=".7 .3 .6 1" size="0.05 0.174" pos="0 0 -0.174" type="capsule"/>

        <body name="l_leg" pos="0 0 -0.348">
          <joint axis="0 1 0" name="l_knee" pos="0 0 0" range="0 150" type="hinge"/>
          <geom friction="0.9" name="leg_left_geom" rgba=".7 .3 .6 1" pos="0 0 -0.1775" size="0.04 0.1775" type="capsule"/>

          <body name="l_foot" pos="0 0 -0.355">
            <joint axis="0 1 0" name="l_ankle" pos="0 0 0" range="-45 45" type="hinge"/>
            <geom friction="1.9" name="foot_left_geom" rgba=".7 .3 .6 1" size="0.06 0.1" pos="0.05 0 -0.08" type="capsule" quat="0.70710678118654757 0 -0.70710678118654746 0"/>
          </body>
        </body>
      </body>
    </body>
  </worldbody>
  <actuator>
    <!-- <motor joint="torso_joint" ctrlrange="-100.0 100.0" isctrllimited="true"/>-->
    <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="100" joint="r_hip"/>
    <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="100" joint="r_knee"/>
    <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="100" joint="r_ankle"/>
    <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="100" joint="l_hip"/>
    <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="100" joint="l_knee"/>
    <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="100" joint="l_ankle"/>
    <!-- <motor joint="finger2_rot" ctrlrange="-20.0 20.0" isctrllimited="true"/>-->
  </actuator>
    <asset>
        <texture type="skybox" builtin="gradient" rgb1=".4 .5 .6" rgb2="0 0 0" width="100" height="100"/>
        <texture builtin="flat" height="1278" mark="cross" markrgb="1 1 1" name="texgeom" random="0.01" rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" type="cube" width="127"/>
        <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
        <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>
        <material name="geom" texture="texgeom" texuniform="true"/>
        <material name="red_material" rgba="1 0 0 1"/>  # red
        <material name="orange_material" rgba="1 0.5 0 1"/>  # orange
        <material name="blue_material" rgba="0 0 1 1"/>  # blue
        <material name="green_material" rgba="0 1 0 1"/>  # green
        <material name="dark_green_material" rgba="0 0.55 0 1"/>  # dark green
        <material name="yellow_material" rgba="1 1 0 1"/>  # yellow
        <material name="purple_material" rgba="1 0 1 1"/>  # purple
        <material name="pink_material" rgba="1 0.75 0.75 1"/>  # pink
        <material name="white_material" rgba="1 1 1 1"/>  # white
    </asset>
  <keyframe>
    <!-- <key name="init_pose" qpos="0 1.05 0.05
                                -0.4 0.4 -0.05 0.2 0.1 -0.3"
                          qvel="0 0 0 0 0 0 0 0 0" /> -->
    <key name = "init_pose" qpos="0  0.98  0.1  
                              0.16  0.1374  0.00747 -0.76  0.66  0.017"
                        qvel="0 0 0 0 0 0 0 0 0" />
</keyframe>
</mujoco>
